{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src/",
  "monorepo": true,
  "root": "src/",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "src/tsconfig.app.json"
  },
  "projects": {
    "nest-auth": {
      "type": "application",
      "root": "src/",
      "entryFile": "main",
      "sourceRoot": "src",
      "compilerOptions": {
        "tsConfigPath": "src/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "lib/auth",
      "entryFile": "index",
      "sourceRoot": "lib/auth/src",
      "compilerOptions": {
        "tsConfigPath": "lib/auth/tsconfig.lib.json"
      }
    }
  }
}
